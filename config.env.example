# Backend API Configuration
BACKEND_API_URL=http://localhost:3010/api/public/funnels/check-domain

# Frontend Private URL (where valid domains are proxied)
FRONTEND_PRIVATE_URL=http://localhost:3010/api/private/funnels/

# Fallback URL for invalid domains
FALLBACK_URL=https://fallback.yourdomain.com

# Cache Configuration
CACHE_TTL=300  # Cache time in seconds (5 minutes)

# Request Timeout
REQUEST_TIMEOUT=3000  # Timeout in milliseconds (3 seconds)

# Optional: Custom DNS resolver
# DNS_RESOLVER=8.8.8.8 8.8.4.4

# Optional: SSL Configuration (for production)
# SSL_CERTIFICATE_PATH=/path/to/certificate.crt
# SSL_CERTIFICATE_KEY_PATH=/path/to/private.key 